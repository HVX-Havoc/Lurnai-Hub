local success, lib = pcall(loadstring, game:HttpGet("https://raw.githubusercontent.com/HVX-Havoc/Lurnai-Hub/refs/heads/main/Github/UI/LurnaiHubLibary"))

if not success then
    print("Error loading UI Library:", lib)
    return
end

local Library, SaveManager, InterfaceManager = lib()

-- Example usage (create window, tab, folder)
local Window = Library:CreateWindow({
    Title = "Blue UI Example",
    Theme = "BlueSky",
    Size = UDim2.new(0, 600, 0, 400)
})

local MainTab = Window:AddTab({
    Title = "Main Tab",
    Icon = "home"
})

local GameSettingsFolder = MainTab:AddFolder("GameSettingsFolder", {
    Title = "Game Settings",
    Description = "Manage all game-related settings"
})

